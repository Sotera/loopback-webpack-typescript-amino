{
  "name": "loopback-webpack-typescript-amino",
  "version": "0.0.3",
  "description": "A webpack starter for a Loopback server and Angular2 client",
  "main": "server/server.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "rimraf dist && webpack --progress --hide-modules --config client/build/webpack.prod.js",
    "pretest": "jshint .",
    "test": "karma start client/build/karma.conf.js --single-run"
  },
  "keywords": [
    "loopback",
    "typescript",
    "amino",
    "webpack",
    "angular2"
  ],
  "author": "John Reeme <john.reeme@soteradefense.com> (http://soteradefense.com)",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotera/loopback-webpack-typescript-amino.git"
  },
  "bugs": {
    "url": "https://github.com/sotera/loopback-webpack-typescript-amino/issues"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "compression": "^1.6.2",
